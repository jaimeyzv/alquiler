<div class="row">
    <section class="panel">
        <div class="panel-body">
             <div class="form-group">
                <label for="txt_ubicacion" class="col-sm-2 control-label">Título</label>
                <div class="col-sm-10">
                <input type="text" class="form-control" id="txt_ubicacion" placeholder="Título">
                </div>
            </div>
            <div class="form-group">
                <label for="txt_ubicacion" class="col-sm-2 control-label">Contenido</label>
                <div class="col-sm-10">
                <input type="file" class="form-control" id="txt_ubicacion" placeholder="Contenido">
                </div>
            </div>
            <div class="form-group">
                <label for="txt_fecha_nac" class="col-sm-2 control-label">Fecha Hora desde:</label>
                <div class="col-sm-10 input-group date" id='datetimepicker1'>
                    <input type="text" class="form-control" id="txt_fecha_nac" placeholder="Fecha inicio">
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
                </div>
            </div>
            <div class="form-group">
                <label for="txt_fecha_nac" class="col-sm-2 control-label">Fecha Hora hasta:</label>
                <div class="col-sm-10 input-group date" id='datetimepicker1'>
                    <input type="text" class="form-control" id="txt_fecha_nac" placeholder="Fecha fin">
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
                </div>
            </div>
            <div class="form-group">
                <label for="txt_ubicacion" class="col-sm-2 control-label">Ubicación</label>
                <div class="col-sm-10">
                     <select id="sel_distrito" class="form-control">
                        <option value="0">Seleccionar Ubicacion:</option>
                        <option value="1">Pagina Principal</option>
                        <option value="2">Pagina Cliente</option>
                        <option value="3">Pagina Dueño</option>
                    </select>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-default">Guardar</button>
                </div>
            </div>
        </div>
    </section>
</div>